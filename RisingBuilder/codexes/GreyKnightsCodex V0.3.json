{
  "codex_name": "Grey Knights (My Data) - V0.3 Fix",
  "rules": {
    "The Aegis": { "summary": "Unit reduces enemy psychic leadership by -1 within 12\". Re-roll 1s to Deny the Witch (6th) / Psychic Hood effect." },
    "Brotherhood of Psykers": { "summary": "The squad counts as a single Psyker. If the Justicar is alive, use his Ld." },
    "Combat Squads": { "summary": "A 10-man squad may split into two 5-man squads before deployment." },
    "Preferred Enemy (Daemons)": { "summary": "Re-roll to hit vs Daemons in close combat." },
    "Prefered Enemy (Daemons)": { "summary": "Re-roll to hit vs Daemons in close combat." },
    "Deep Strike": { "summary": "May arrive via Deep Strike." },
    "Warp Quake": { "summary": "Psychic Power. Enemies Deep Striking within 12\" suffer a mishap automatically." },
    "Hammerhand": { "summary": "Psychic Power. Unit gains +1 Strength in combat." },
    "Might of Titan": { "summary": "Psychic Power. Unit gains +1 Strength and +1d6 Armor Pen." },
    "Sanctuary": { "summary": "Psychic Power. Enemies assaulting the unit treat open ground as dangerous terrain." },
    "Cleansing Flame": { "summary": "Psychic Power. S5 AP4 hits on all enemy models in base contact." },
    "Grand Strategy": { "summary": "D3 units gain Scout, Counter-attack, or Scoring." },
    "Psychic Pilot": { "summary": "Vehicle counts as a Psyker." },
    "Fortitude": { "summary": "Psychic Power. Vehicle removes Shaken or Stunned result." },
    "Independent Character": { "summary": "May join/leave units. Look Out Sir rolls." },
    "Psyker (Mastery 1)": { "summary": "Generates 1 Warp Charge per turn." },
    "Psyker (Level 1)": { "summary": "Generates 1 Warp Charge per turn." },
    "Heroic Sacrifice": { "summary": "If killed in melee, the model may make a single attack against the slayer." },
    "Master Swordsman": { "summary": "Re-roll failed To Hit rolls in close combat." },
    "Fearless": { "summary": "Passes Morale/Pinning automatically. Cannot go to Ground." },
    "Holocaust": { "summary": "Psychic Power. Large Blast S5 AP- range 12\"." },
    "Assault Vehicle": { "summary": "Passengers may assault the turn they disembark." }
  },
  "wargear": {
    "Personal Teleporter": { "summary": "Unit moves as Jump Infantry. Once per game 30\" shunt move." },
    "Terminator Armour": { "summary": "2+ Armour Save, 5+ Invulnerable Save. Relentless, Bulky, Deep Strike." },
    "Iron Halo": { "summary": "4+ Invulnerable Save." },
    "Psychic Hood": { "summary": "Attempt to nullify enemy psychic powers within 24\"." },
