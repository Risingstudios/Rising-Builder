{
  "codex_name": "Orks (5th Edition) - V0.4",
  "rules": {
    "Prophet of the Waaagh!": { "summary": "Ghazghkull gives the whole army Fearless for a turn. He gets a 2+ Invuln save that turn." },
    "One Scalpel Short of a Medpack": { "summary": "Grotsnik is Fearless and has Rampage. He must always move towards the nearest enemy." },
    "Zog's Curse": { "summary": "Psychic Power. Turn an enemy independent character into a Squig on a roll of 4+." },
    "Da Vulcha's Klaws": { "summary": "Zagstruk strikes at Initiative 1, but on the turn he Deep Strikes, he strikes at Initiative 4." },
    "Ambush": { "summary": "Snikrot and his unit can move on from any table edge." },
    "Mob Rule": { "summary": "Units can use the number of models in the squad as their Leadership value (Max 10). If >10 models, they are Fearless." },
    "Furious Charge": { "summary": "+1 Strength and +1 Initiative on the turn they assault." },
    "Waaagh!": { "summary": "Once per game, declare Waaagh! Infantry units gain Fleet. If they roll a 1 for Fleet, they take a wound. Affects Run moves." },
    "Ramshackle": { "summary": "When vehicle is wrecked, roll on Ramshackle table. It might careen forward or explode harmlessly." },
    "Power of the Waaagh!": { "summary": "Weirdboyz determine psychic power randomly at start of turn by rolling D6." },
    "Eere We Go!": { "summary": "Deep Striking unit ignores dangerous terrain tests. If they scatter onto impassable/units, reduce scatter distance to avoid mishap." },
    "Boarding Planks": { "summary": "Models can assault from this vehicle even if it moved (up to 12\"). Adds 2\" to disembark/assault range." },
    "Independent Character": { "summary": "May join/leave units. Look Out Sir rolls." },
    "Eternal Warrior": { "summary": "Immune to Instant Death." },
    "Fearless": { "summary": "Passes Morale/Pinning automatically. Cannot go to Ground." },
    "Feel No Pain": { "summary": "Ignore unsaved wounds on 4+ (unless Instant Death/AP1/AP2)." },
    "Psyker": { "summary": "Can use Psychic Powers." },
    "Deep Strike": { "summary": "May deploy via Deep Strike rules." },
    "Move Through Cover": { "summary": "Roll 3D6 for cover movement, pick highest." },
    "Scouts": { "summary": "May Outflank. After deployment, may make a free move." },
    "Hit and Run": { "summary": "End of assault phase: roll Initiative to break from combat and move 3D6\"." },
    "It's a Grot!": { "summary": "Gretchin do not cause Panic tests for non-Grot units." }
  },
  "wargear": {
    "Adamantium Skull": { "summary": "Ghazghkull has a 2+ Invulnerable save on the turn he calls the Waaagh!" },
    "Dok's Tools": { "summary": "Counts as a Poisoned (4+) weapon." },
    "Bike of the Aporkalypse": { "summary": "Warbike. Can fire the Dakkacannon (S8 AP4 Assault 4) and still assault." },
    "Da Ripper": { "summary": "Badrukk's Gun: Range 24\" S7 AP2 Assault 3, Gets Hot." },
    "Goldtoof Armour": { "summary": "3+ Armor Save. 5+ Invulnerable Save (improves to 3+ vs Shooting)." },
    "Nest of Vipers": { "summary": "Zogwort attacks with 2D6 poison attacks in melee." },
    "Bosspole": { "summary": "Allows unit to re-roll failed Morale/Pinning tests. Usually requires taking a wound." },
    "Nob: Bosspole": { "summary": "Allows unit to re-roll failed Morale/Pinning tests. Usually requires taking a wound." },
    "Cybork Body": { "summary": "5+ Invulnerable Save." },
    "Cybork Bodies (5+ Inv)": { "summary": "5+ Invulnerable Save." },
    "Attack Squig": { "summary": "Adds +1 Attack to profile." },
    "Kustom Force Field": { "summary": "5+ Cover Save for all units within 6\"." },
    "Grot Oiler": { "summary": "Re-roll a repair roll." },
    "Grot Oiler (Max 3)": { "summary": "Re-roll a repair roll." },
    "Ammo Runt": { "summary": "Re-roll one To Hit roll shooting." },
    "Mega Armour": { "summary": "2+ Armor Save, Relentless, Slow and Purposeful. Includes Power Klaw and Twin-linked Shoota." },
    "Mega Armour (2+ Sv / PK / TL-Shoota)": { "summary": "2+ Armor Save, Relentless, Slow and Purposeful. Includes Power Klaw and Twin-linked Shoota." },
    "Stikkbombs": { "summary": "Frag grenades (strike at initiative in cover)." },
    "Tankbusta Bombs": { "summary": "S8 AP- melee attacks vs vehicles." },
    "Deff Rolla": { "summary": "Tank Shock causes D6 S10 hits." },
    "Grot Riggers": { "summary": "Vehicle has 'It Will Not Die' equivalent (Fix Immobilised on 6)." },
    "Red Paint Job": { "summary": "+1\" movement." },
    "Mek's Tools": { "summary": "Can repair vehicles in base contact on a 4+ (or 3+ with Grot Oiler)." },
    "'Eavy Armour": { "summary": "Grants a 4+ Armour Save." },
    "'Eavy Armour (4+ Sv)": { "summary": "Grants a 4+ Armour Save." },
    "Nob: 'Eavy Armour": { "summary": "Grants a 4+ Armour Save." },
    "Warbike (+1T / 4+ Cover)": { "summary": "Unit becomes Biker. +1 Toughness, 4+ Cover Save, Exhaust Cloud." },
    "Warbikes (Entire Squad)": { "summary": "Unit mounts Warbikes." },
    "Waaagh! Banner": { "summary": "+1 WS to the unit." },
    "Jump Pack": { "summary": "Move 12\". Can Deep Strike." },
    "Runtherd": { "summary": "Character. Keeps Gretchin in check." },
    "Squig Hound": { "summary": "Attack Squig (+1A) or Herd Squig (Reroll Morale)." },
    "Grot Prod": { "summary": "Counts as Close Combat Weapon. Doubles Runtherd Strength for discipline." },
    "Armour Plates": { "summary": "Stunned results on damage chart count as Shaken instead." },
    "Reinforced Ram": { "summary": "Reroll Dangerous Terrain tests. +2 Front Armour when Tank Shocking." },
    "Bigbomm": { "summary": "One use. Dropped in movement phase. Large Blast, S5 AP5." }
  },
