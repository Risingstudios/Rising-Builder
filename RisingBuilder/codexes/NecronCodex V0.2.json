    "Fearless": { "summary": "Passes Morale/Pinning automatically." },
    "Ignores Terrain": { "summary": "Treats difficult terrain as open ground." },
    "Wraithflight (Move through anything)": { "summary": "Moves through models and terrain without penalty." },
    "Swarm": { "summary": "Vulnerable to Blasts/Templates. Double wounds." },
    "Eternity Gate (Teleport units)": { "summary": "Can pull a unit from reserves or the board to the Monolith's door." },
    "Construct": { "summary": "Automated creature." },
