{
  "codex_name": "Tau Empire (My Data)",
  "rules": {
    "Acute Senses": { "summary": "Re-roll Night Fighting spotting distance checks." },
    "Deep Strike": { "summary": "Unit may start in reserve and arrive via Deep Strike." },
    "Infiltrate": { "summary": "Unit may deploy anywhere not within 12\" of an enemy (18\" if in sight)." },
    "Fleet": { "summary": "Unit may Run and Assault in the same turn (if applicable) or Run D6\"." },
    "Jet Pack": { "summary": "Moves 6\" in Movement. Can move 6\" in Assault phase (Jet Pack move)." },
    "Scouts": { "summary": "May Outflank. After deployment, may make a free move." },
    "Stealth Field": { "summary": "Night Fighting rules apply to enemies shooting at this unit. +1 Cover Save." },
    "Slow and Purposeful": { "summary": "Move as through difficult terrain. Count as stationary for shooting heavy weapons. Cannot Run/Sweeping Advance." },
    "Markerlight": { "summary": "Heavy 1 weapon. No damage. Place a counter on target. Counters can be spent to boost BS, ignore cover, or launch Seeker Missiles." },
    "Supporting Fire": { "summary": "Not a 5th ed rule, but Photon Grenades provide defensive bonuses." },
    "Bonding Knife": { "summary": "Unit may regroup even if below 50% strength." },
    "Hit and Run": { "summary": "At end of assault phase, may choose to leave combat (roll Initiative check)." },
    "Independent Character": { "summary": "May join/leave units." },
    "Fearless": { "summary": "Automatically pass Morale and Pinning tests. Cannot go to Ground." },
    "Relentless": { "summary": "Can move and shoot Heavy weapons. Can charge after firing Rapid Fire/Heavy weapons." },
    "Move Through Cover": { "summary": "Roll 3D6 for cover move, pick highest." },
    "Ethereal": { "summary": "Friendly Tau within 12\" may re-roll Morale/Pinning. If Ethereal is killed, all Tau must test Morale." },
    "Inspiring Presence": { "summary": "Friendly units within 12\" can use the Ethereal's Leadership." },
    "Supreme Commander": { "summary": "Aun'Va allows all Tau on the table to re-roll failed Morale, Pinning, and Regroup tests." }
  },
  "wargear": {
    "Battlesuit": { "summary": "3+ Armor Save. Multi-tracker, Blacksun Filter built-in. Counts as Jet Pack infantry." },
    "Combat Armour": { "summary": "4+ Armor Save." },
    "Stealth Armour": { "summary": "3+ Armor Save. Stealth Field Generator." },
    "Shield Generator": { "summary": "4+ Invulnerable Save." },
    "Drone Controller": { "summary": "Allows the model to take Drones." },
    "Multi-tracker": { "summary": "Model may fire two weapons in the same Shooting phase." },
    "Target Lock": { "summary": "Model may fire at a different target than the rest of the unit." },
    "Blacksun Filter": { "summary": "Ignore Night Fighting rules." },
    "Advanced Stabilisation": { "summary": "Slow and Purposeful (Move and shoot Heavy weapons)." },
    "Stimulant Injector": { "summary": "Feel No Pain (Ignore wounds on 4+)." },
    "Positional Relay": { "summary": "Allows reserve rolls to be manipulated (bring on a unit on 2+)." },
    "Flechette Discharger": { "summary": "Enemies assaulting vehicle take S4 hits." },
    "Disruption Pod": { "summary": "Vehicle counts as obscured (4+ cover) if > 12\" away." },
    "Decoy Launchers": { "summary": "Convert 'Immobilised' result to 'Crew Stunned' on 4+." },
    "Landing Gear": { "summary": "Vehicle may land." },
    "Networked Markerlight": { "summary": "Markerlight that can be used by the unit firing it." },
    "Photon Grenades": { "summary": "Defensive grenades. Enemies do not get +1 A for charging." },
    "EMP Grenades": { "summary": "Haywire grenades. Glancing on 2-5, Penetrating on 6 against vehicles." },
    "Command-link Drone": { "summary": "Allows one friendly unit within 12\" to re-roll 1s to Hit." },
    "Advanced Shield Drone": { "summary": "3+ Invulnerable Save." },
    "Paradox of Duality": { "summary": "Aun'Va has a 5+ Invulnerable save. If failed, roll D6: on 5+, the hit is ignored." }
  },
