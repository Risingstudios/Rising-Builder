{
  "codex_name": "Tau Empire (My Data) - V1.1",
  "rules": {
    "Acute Senses": { "summary": "Re-roll Night Fighting spotting distance checks." },
    "Deep Strike": { "summary": "Unit may start in reserve and arrive via Deep Strike." },
    "Infiltrate": { "summary": "Unit may deploy anywhere not within 12\" of an enemy (18\" if in sight)." },
    "Fleet": { "summary": "Unit may Run and Assault in the same turn (if applicable) or Run D6\"." },
    "Jet Pack": { "summary": "Moves 6\" in Movement. Can move 6\" in Assault phase (Jet Pack move)." },
    "Scouts": { "summary": "May Outflank. After deployment, may make a free move." },
    "Stealth Field": { "summary": "Night Fighting rules apply to enemies shooting at this unit. +1 Cover Save." },
    "Slow and Purposeful": { "summary": "Move as through difficult terrain. Count as stationary for shooting heavy weapons. Cannot Run/Sweeping Advance." },
    "Markerlight": { "summary": "Heavy 1 weapon. No damage. Place a counter on target. Counters can be spent to boost BS, ignore cover, or launch Seeker Missiles." },
    "Supporting Fire": { "summary": "Not a 5th ed rule, but Photon Grenades provide defensive bonuses." },
    "Bonding Knife": { "summary": "Unit may regroup even if below 50% strength." },
    "Hit and Run": { "summary": "At end of assault phase, roll Initiative to break combat and move 3D6\"." },
    "Preferred Enemy (Orks)": { "summary": "Re-roll failed To Hit rolls against Orks in close combat." },
    "Skilled Rider": { "summary": "+1 to Jink cover saves. Re-roll dangerous terrain tests." },
    "Pinning": { "summary": "If unit suffers a wound from this weapon, it must take a Leadership test or Go to Ground." }
  },
  "wargear": {
    "Target lock": { "summary": "Model can fire at a different target to the rest of the unit." },
    "Shield generator": { "summary": "Model has a 4+ Invulnerable Save." },
    "Stimulant injector": { "summary": "Model has Feel No Pain." },
    "Gun Drone": { "summary": "Drone equipped with Twin-linked Pulse Carbine. Counts as Jet Pack Infantry." },
    "Two Gun Drones": { "summary": "Vehicle is equipped with two detachable Gun Drones." },
    "Shield Drone": { "summary": "Drone equipped with a Shield Generator (4+ Invulnerable Save)." },
    "Marker Drone": { "summary": "Drone equipped with a Networked Markerlight." },
    "Positional Relay": { "summary": "If model is on board, +1 to Reserve rolls." },
    "Command Link Drone": { "summary": "Allows a Commander to fire at a different target and share leadership." },
    "Drone Controller": { "summary": "Allows the model to control drones." },
    "Blacksun Filter": { "summary": "Ignore Night Fighting rules." },
    "Multi-tracker": { "summary": "Model can fire two weapons in the Shooting phase." },
    "Hard-wired Multi-tracker": { "summary": "Model can fire two weapons in the Shooting phase." },
    "Hard-wired Target Lock": { "summary": "Model can fire at a different target." },
    "Advanced Target Lock": { "summary": "Model can fire at a different target." },
    "Iridium Armour": { "summary": "2+ Armor Save. Reduced movement." },
    "Vectored Retro-thrusters": { "summary": "Model has Hit and Run." },
    "Ejection System": { "summary": "If vehicle wrecked, crew can bail out safely." },
    "Flechette Discharger": { "summary": "Enemy units assaulting vehicle take hits." },
    "Decoy Launchers": { "summary": "Vehicle has 4+ invulnerable save against glancing hits?" },
    "Sensor Spines": { "summary": "Vehicle ignores difficult terrain." },
    "Disruption Pod": { "summary": "Vehicle is obscured (4+ Cover Save) if over 12\" away." },
    "Crisis Battlesuit": { "summary": "3+ Armor Save, Jet Pack. Includes Multi-tracker and Blacksun Filter." },
    "XV22 Stealth Battlesuit": { "summary": "3+ Armor Save, Stealth Field, Jet Pack." },
    "Symbol of Office": { "summary": "Ethereal allows friendly Tau units within 12\" to re-roll Morale tests." },
    "Homing Beacon": { "summary": "Friendly units Deep Striking within 6\" do not scatter." },
    "Kroot Hound": { "summary": "Beasts. Initiative 4, 2 Attacks." },
    "Krootox Rider": { "summary": "Krootox (S6 T5 W3) with Kroot Gun." },
    "Kroot Armour (6+ Sv)": { "summary": "Unit gains a 6+ Armor Save." },
    "Replace Burst Cannons with 2 Gun Drones": { "summary": "Swap Burst Cannons for two detachable Gun Drones." },
    "Six Seeker Missiles": { "summary": "Vehicle carries 6 Seeker Missiles." },
    "Two Fusion Blasters": { "summary": "Two Fusion Blasters." }
  },
