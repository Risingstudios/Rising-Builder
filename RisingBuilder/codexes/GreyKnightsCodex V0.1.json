{
  "codex_name": "Grey Knights (My Data)",
  "rules": {
    "The Aegis": { "summary": "Unit reduces enemy psychic leadership by -1 within 12\". Re-roll 1s to Deny the Witch (6th) / Psychic Hood effect." },
    "Brotherhood of Psykers": { "summary": "The squad counts as a single Psyker. If the Justicar is alive, use his Ld." },
    "Combat Squads": { "summary": "A 10-man squad may split into two 5-man squads before deployment." },
    "Preferred Enemy (Daemons)": { "summary": "Re-roll to hit vs Daemons in close combat." },
    "Deep Strike": { "summary": "May arrive via Deep Strike." },
    "Warp Quake": { "summary": "Psychic Power. Enemies Deep Striking within 12\" suffer a mishap automatically." },
    "Hammerhand": { "summary": "Psychic Power. Unit gains +1 Strength in combat." },
    "Might of Titan": { "summary": "Psychic Power. Unit gains +1 Strength and +1d6 Armor Pen." },
    "Sanctuary": { "summary": "Psychic Power. Enemies assaulting the unit treat open ground as dangerous terrain." },
    "Cleansing Flame": { "summary": "Psychic Power. S5 AP4 hits on all enemy models in base contact." },
    "Grand Strategy": { "summary": "D3 units gain Scout, Counter-attack, or Scoring." },
    "Psychic Pilot": { "summary": "Vehicle counts as a Psyker." },
    "Fortitude": { "summary": "Psychic Power. Vehicle ignores Shaken/Stunned results." }
  },
  "wargear": {
    "Nemesis Force Sword": { "summary": "Force Weapon. +1 to Invulnerable Save in combat." },
    "Nemesis Force Halberd": { "summary": "Force Weapon. +2 Initiative." },
    "Nemesis Warding Stave": { "summary": "Force Weapon. Wielder has a 2+ Invulnerable Save in combat." },
    "Nemesis Daemon Hammer": { "summary": "Force Weapon. Sx2, AP2, Concussive (same as Thunder Hammer)." },
    "Nemesis Falchions": { "summary": "Force Weapon. +1 Attack." },
    "Nemesis Greatsword": { "summary": "Force Weapon. Re-roll to hit and to wound." },
    "Storm Shield": { "summary": "3+ Invulnerable Save." },
    "Personal Teleporter": { "summary": "Unit moves like Jump Infantry. Once per game can make a 30\" shunt move." },
    "Psychotroke Grenades": { "summary": "Random effect on enemy unit in assault (lose attacks, attack selves, etc)." },
    "Rad Grenades": { "summary": "-1 Toughness to enemy unit in assault." },
    "Psybolt Ammunition": { "summary": "+1 Strength to Bolter/Autocannon weapons." }
  },
  "weapons": {
    "Storm bolter": { "range": "24\"", "S": "4", "AP": "5", "type": "Assault 2", "notes": "" },
    "Psycannon": { "range": "24\"", "S": "7", "AP": "4", "type": "Assault 2/Heavy 4", "notes": "Rending" },
    "Incinerator": { "range": "Template", "S": "5", "AP": "4", "type": "Assault 1", "notes": "Ignores Cover" },
    "Psilencer": { "range": "24\"", "S": "4", "AP": "-", "type": "Heavy 6", "notes": "Force Weapon (Wounds Daemons on 2+)" },
    "Heavy Psycannon": { "range": "24\"", "S": "7", "AP": "4", "type": "Heavy 1", "notes": "Large Blast" },
    "Heavy Incinerator": { "range": "Template", "S": "6", "AP": "4", "type": "Heavy 1", "notes": "" },
    "Mindstrike Missiles": { "range": "Unlimited", "S": "4", "AP": "5", "type": "Heavy 1", "notes": "Blast. Psykers take Perils on hit." }
  },
